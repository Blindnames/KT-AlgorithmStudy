    "data = input()\n",
    "cha = []\n",
    "sum = 0\n",
    "cnt = 0\n",
    "for i in data:\n",
    "    if i.isalpha():\n",
    "        cha.append(i)\n",
    "    else :\n",
    "        sum += int(i)\n",
    "        cnt +=1\n",
    "cha.sort()\n",
    "\n",
    "if cnt != 0:\n",
    "    cha.append(str(sum))\n",
    "\n",
    "for i in range(len(cha)):\n",
    "    print(cha[i] , end='')"
   ]
  }
